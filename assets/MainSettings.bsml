<bg id='root' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:noNamespaceSchemaLocation='https://raw.githubusercontent.com/RedBrumbler/Quest-BSML-Docs/gh-pages/schema.xsd'>
    <vertical pref-height='80' vertical-fit='PreferredSize' pref-width='90' horizontal-fit='PreferredSize'>
        <text text="Toggle Settings" font-size='6'/>
        <toggle-setting label='Enable First Timed Switch' value='firstTime'/>
        <toggle-setting label='Enable Second Timed Switch' value='secondTime'/>
        <toggle-setting label='Enable Third Timed Switch' value='thirdTime'/>
        <toggle-setting label='Enable Fourth Timed Switch' value='fourthTime'/>
        <color-setting text='Left Saber Colour' apply-on-change='true' value='LeftSaberColour' hover-hint='Changes the colour of the left saber'/>
    </vertical>
</bg>